﻿@model IEnumerable<ProjectWeb.Models.EF.Order>
@if (Model != null)
{
    var i = 1;
    <h2>Lịch sử mua hàng</h2>
    <table class="table table-bordered">
        <thead>
            <tr>
                <td>STT</td>              
                
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@i</td>
                    
                </tr>
                i++;
            }
        </tbody>
    </table>
}
